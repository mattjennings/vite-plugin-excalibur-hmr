{
  "name": "vite-plugin-excalibur-hmr",
  "version": "0.0.1",
  "description": "Adds Scene hot module reloading to Excalibur",
  "type": "module",
  "files": [
    "src"
  ],
  "exports": {
    "./hot": {
      "types": "./src/hot.d.ts",
      "default": "./src/hot.js"
    },
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*"
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "excalibur": "^0.28.3",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "excalibur": "^0.28.3",
    "vite": ">=4"
  }
}
